<html>
        <head> </head>

        <body>
                <canvas id="myCanvas" width="1400" height="1400">
                
                <script>
                        var c = document.getElementById("myCanvas");
                        var ctx = c.getContext("2d");

                        ctx.lineWidth="0";
                        ctx.lineCap="butt";
                        ctx.lineJoin="bevel";
			ctx.beginPath();
                        //ctx.moveTo(150, 100);
			ctx.moveTo(100, 100);
                        ctx.lineTo(200, 100);
                        ctx.lineTo(200, 200);
                        ctx.lineTo(100, 200);
                        ctx.lineTo(100, 100);
			//ctx.lineTo(150, 100);
                        //ctx.closePath();

                        ctx.stroke();
                        //ctx.fill();


                        ctx.lineWidth="100";
                        ctx.lineCap="butt";
                        ctx.lineJoin="miter";
			ctx.beginPath();
                        ctx.moveTo(150, 300);
			//ctx.moveTo(100, 300);
                        ctx.lineTo(200, 300);
                        ctx.lineTo(200, 400);
                        ctx.lineTo(100, 400);
                        ctx.lineTo(100, 300);
			ctx.lineTo(150, 300);
                        //ctx.closePath();

                        ctx.stroke();
                        //ctx.fill();

                        ctx.lineWidth="100";
                        ctx.lineCap="butt";
                        ctx.lineJoin="round";
			ctx.beginPath();
                        //ctx.moveTo(100, 500);
			ctx.moveTo(150, 500);
                        ctx.lineTo(200, 500);
                        ctx.lineTo(200, 600);
                        ctx.lineTo(100, 600);
                        ctx.lineTo(100, 500);
			ctx.lineTo(150, 500);
                        //ctx.closePath();

                        ctx.stroke();
                        //ctx.fill();

                </script>
        </body>
</html> 
